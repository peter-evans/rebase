{
  "name": "rebase",
  "version": "1.0.0",
  "private": true,
  "description": "Rebase pull requests in a repository",
  "main": "lib/main.js",
  "scripts": {
    "vendor": "rm -rf src/vendor/checkout && git clone --depth=1 --branch=v2.3.4 http://github.com/actions/checkout src/vendor/checkout && rm -rf src/vendor/checkout/.git",
    "clean": "rm -rf node_modules package-lock.json",
    "build-checkout": "npm run build --prefix src/vendor/checkout",
    "build": "tsc && ncc build",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peter-evans/rebase.git"
  },
  "keywords": [
    "actions",
    "pull",
    "rebase"
  ],
  "author": "Peter Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peter-evans/rebase/issues"
  },
  "homepage": "https://github.com/peter-evans/rebase",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/io": "1.1.1",
    "@octokit/graphql": "4.8.0",
    "checkout": "file:src/vendor/checkout",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "17.0.12",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "@vercel/ncc": "0.33.1",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-github": "4.3.5",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.7",
    "jest-circus": "27.4.6",
    "js-yaml": "4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
}
